CREATE DEFINER=`covmo`@`%` PROCEDURE `SP_Check_NT_DB`(IN NTDATE VARCHAR(18))
BEGIN
	DECLARE NT_DB VARCHAR(50) DEFAULT CONCAT('gt_nt_',CONCAT(LEFT(NTDATE,8)));
			
	SELECT COUNT(*) INTO @CNT FROM `information_schema`.`SCHEMATA` WHERE `SCHEMA_NAME` = NT_DB;
	
	IF @CNT != 1 THEN
		SELECT 1 AS Message;
	ELSE
		SELECT 0 AS Message;
	END IF;
	
END
