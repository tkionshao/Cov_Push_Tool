CREATE DEFINER=`covmo`@`%` PROCEDURE `SP_CreateDB_NT_Schema`(IN GT_DB VARCHAR(100))
BEGIN
	DECLARE extarTables VARCHAR(50);
	SELECT LOWER(`value`) INTO extarTables  FROM gt_gw_main.integration_param WHERE gt_group = 'ntparser' AND gt_name = 'extraTables' ;
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_antenna`
	 (
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` mediumint(9) DEFAULT NULL,
	  `CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LONGITUDE` double DEFAULT NULL,
	  `LATITUDE` double DEFAULT NULL,
	  `AZIMUTH` SMALLINT(6) DEFAULT NULL,
	  `ANTENNA_MODEL` varchar(50) DEFAULT NULL,
	  `ANTENNA_TYPE` tinyint(4) DEFAULT NULL,
	  `HEIGHT` double DEFAULT NULL,
	  `BEAMWIDTH_H` double DEFAULT NULL,
	  `BEAMWIDTH_V` double DEFAULT NULL,
	  `CPICH_POWER` double DEFAULT NULL,
	  `EIRP` double DEFAULT NULL,
	  `ANTENNA_GAIN` double DEFAULT NULL,
	  `FEEDER_LOSS` double DEFAULT NULL,
	  `DOWNTILT_EL` double DEFAULT NULL,
	  `DOWNTILT_MEC` double DEFAULT NULL,
	  `ELEVATION` double DEFAULT NULL,
	  `INDOOR_TYPE` TINYINT(4) DEFAULT 0,
	  `REPEATER_TYPE` VARCHAR(20) DEFAULT NULL,
	  `REPEATER_PG_DELAY` SMALLINT(6) DEFAULT NULL,
	  `ANTENNA_ID` TINYINT(4) DEFAULT 1,
	  `PATHLOSS_DISTANCE` float DEFAULT NULL,
	  `ANTENNA_RADIUS` float DEFAULT NULL,
	  `CLOSED_RADIUS` float DEFAULT NULL,
	  `FLAG` SMALLINT(6) DEFAULT ''0'',
	  KEY `ID_KEY` (`RNC_ID`,`CELL_ID`)
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_antenna_current`
	(         
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` mediumint(9) DEFAULT NULL,
	  `CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LONGITUDE` double DEFAULT NULL,
	  `LATITUDE` double DEFAULT NULL,
	  `AZIMUTH` SMALLINT(6) DEFAULT NULL,
	  `ANTENNA_MODEL` varchar(50) DEFAULT NULL,
	  `ANTENNA_TYPE` tinyint(4) DEFAULT NULL,
	  `HEIGHT` double DEFAULT NULL,
	  `BEAMWIDTH_H` double DEFAULT NULL,
	  `BEAMWIDTH_V` double DEFAULT NULL,
	  `CPICH_POWER` double DEFAULT NULL,
	  `EIRP` double DEFAULT NULL,
	  `ANTENNA_GAIN` double DEFAULT NULL,
	  `FEEDER_LOSS` double DEFAULT NULL,
	  `DOWNTILT_EL` double DEFAULT NULL,
	  `DOWNTILT_MEC` double DEFAULT NULL,
	  `ELEVATION` double DEFAULT NULL,
	  `INDOOR_TYPE` TINYINT(4) DEFAULT 0,
	  `REPEATER_TYPE` VARCHAR(20) DEFAULT NULL,
	  `REPEATER_PG_DELAY` SMALLINT(6) DEFAULT NULL,
	  `ANTENNA_ID` TINYINT(4) DEFAULT 1,
	  `PATHLOSS_DISTANCE` float DEFAULT NULL,
	  `ANTENNA_RADIUS` float DEFAULT NULL,
	  `CLOSED_RADIUS` float DEFAULT NULL,
	  `FLAG` SMALLINT(6) DEFAULT ''0'',
	   KEY `RNC_ID` (`RNC_ID`,`CELL_ID`),
	   KEY `ANTENNA_MODEL`( `ANTENNA_MODEL`)
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;  
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_cell`
	 (
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `SITE_ID` varchar(50) DEFAULT NULL,
	  `SITE_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LAC` mediumint(9) unsigned DEFAULT NULL,
	  `RAC` SMALLINT(6) unsigned DEFAULT NULL,
	  `CELL_ID` mediumint(9) DEFAULT NULL,
	  `CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `SCRAMBLING_CODE` smallint(6) DEFAULT NULL,
	  `FREQUENCY` smallint(6) DEFAULT NULL,
	  `DL_UARFCN` mediumint(9) DEFAULT NULL,
	  `LONGITUDE` double DEFAULT NULL,
	  `LATITUDE` double DEFAULT NULL,
	  `INDOOR` tinyint(4) not null DEFAULT ''0'',
	  `SITE_TYPE` varchar(20) DEFAULT NULL,
	  `ACTIVE_STATUS` varchar(30) DEFAULT NULL,
	  `HSUPA_ACT` varchar(50) DEFAULT NULL,
	  `HSUDPA_ACT` varchar(50) DEFAULT NULL,
	  `CLUSTER_NAME` varchar(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `CLUSTER_NAME_REGION` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `CLUSTER_NAME_SUB_REGION` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `CLUSTER_NAME_ZONE` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LOCAL_SITE_ID` mediumint(9) DEFAULT NULL,
	  `LOCAL_SITE_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `NumHSCode` smallint(6) DEFAULT NULL,
	  `ADMINSTATE_LOCKED` tinyint(4) DEFAULT NULL,
	  `OPERSTATE_ENABLE` tinyint(4) DEFAULT NULL,
	  `WCEL_INDEX` MEDIUMINT(9) DEFAULT NULL,
	  `CLUSTER_ID` mediumint(9) DEFAULT NULL,
	  `INAFREQ_NBCNT` tinyint(4) unsigned DEFAULT NULL,
	  `INTFREQ_NBCNT` tinyint(4) unsigned DEFAULT NULL,
	  `GSM_NBCNT` tinyint(4) unsigned DEFAULT NULL,
	  `NB_AVG_DISTANCE_VORONOI` double DEFAULT NULL,
	  `NB_AVG_DISTANCE_UMTS` double DEFAULT NULL,
	  `SITE_DENSITY_TYPE` tinyint(4) DEFAULT NULL,
	  `FLAG` SMALLINT(6) DEFAULT ''0'',
	SERVING_AREA VARCHAR(20) CHARACTER SET UTF8 DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_current`
	 (
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `SITE_ID` varchar(20) DEFAULT NULL,
	  `SITE_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LAC` mediumint(9) unsigned DEFAULT NULL,
	  `RAC` SMALLINT(6) unsigned DEFAULT NULL,
	  `CELL_ID` mediumint(9) DEFAULT NULL,
	  `CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `SCRAMBLING_CODE` smallint(6) DEFAULT NULL,
	  `FREQUENCY` smallint(6) DEFAULT NULL,
	  `DL_UARFCN` mediumint(9) DEFAULT NULL,
	  `LONGITUDE` double DEFAULT NULL,
	  `LATITUDE` double DEFAULT NULL,
	  `INDOOR` tinyint(4) not null DEFAULT ''0'',
	  `SITE_TYPE` varchar(20) DEFAULT NULL,
	  `ACTIVE_STATUS` varchar(30) DEFAULT NULL,
	  `HSUPA_ACT` varchar(50) DEFAULT NULL,
	  `HSUDPA_ACT` varchar(50) DEFAULT NULL,
	  `CLUSTER_NAME` varchar(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `CLUSTER_NAME_REGION` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `CLUSTER_NAME_SUB_REGION` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `CLUSTER_NAME_ZONE` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LOCAL_SITE_ID` mediumint(9) DEFAULT NULL,
	  `LOCAL_SITE_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `NumHSCode` smallint(6) DEFAULT NULL,
	  `ADMINSTATE_LOCKED` tinyint(4) DEFAULT NULL,
	  `OPERSTATE_ENABLE` tinyint(4) DEFAULT NULL,
	  `WCEL_INDEX` MEDIUMINT(9) DEFAULT NULL,
	  `CLUSTER_ID` mediumint(9) DEFAULT NULL,
	  `INAFREQ_NBCNT` tinyint(4) unsigned DEFAULT NULL,
	  `INTFREQ_NBCNT` tinyint(4) unsigned DEFAULT NULL,
	  `GSM_NBCNT` tinyint(4) unsigned DEFAULT NULL,
	  `NB_AVG_DISTANCE_VORONOI` double DEFAULT NULL,
	  `NB_AVG_DISTANCE_UMTS` double DEFAULT NULL,
	  `SITE_DENSITY_TYPE` tinyint(4) DEFAULT NULL,
	  `FLAG` SMALLINT(6) DEFAULT ''0'',
	SERVING_AREA VARCHAR(20) CHARACTER SET UTF8 DEFAULT NULL,
	KEY `NewIndex2` (`RNC_ID`,`CELL_ID`),
	KEY `CLUSTER_NAME` (`CLUSTER_NAME`),
	KEY `IX_INDOOR` (`INDOOR`),
	KEY `IX_IDX1` (`SITE_ID`,`RNC_ID`)
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_neighbor`
	(
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` mediumint(9) DEFAULT NULL,
	  `NBR_RNC_ID` int(11) DEFAULT NULL,
	  `NBR_RNC_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `NBR_CELL_ID` mediumint(9) DEFAULT NULL,
	  `NBR_CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `PRIORITY` tinyint(4) DEFAULT NULL,
	  `NBR_TYPE` tinyint(4) DEFAULT NULL,
	  `NBR_BCCH_ARFCN` varchar(50) DEFAULT NULL,
	  `NBR_BSIC` varchar(20) DEFAULT NULL,
	  `NBR_DISTANCE` double DEFAULT NULL,
	  `NBR_ANGLE` mediumint(9) DEFAULT NULL,
	  `NBR_AZIMUTH_ANGLE` mediumint(9) DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_neighbor_current`
	(
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` mediumint(9) DEFAULT NULL,
	  `NBR_RNC_ID` int(11) DEFAULT NULL,
	  `NBR_RNC_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `NBR_CELL_ID` mediumint(9) DEFAULT NULL,
	  `NBR_CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `PRIORITY` tinyint(4) DEFAULT NULL,
	  `NBR_TYPE` tinyint(4) DEFAULT NULL,
	  `NBR_BCCH_ARFCN` varchar(50) DEFAULT NULL,
	  `NBR_BSIC` varchar(20) DEFAULT NULL,
	  `NBR_DISTANCE` double DEFAULT NULL,
	  `NBR_ANGLE` mediumint(9) DEFAULT NULL,
	  `NBR_AZIMUTH_ANGLE` mediumint(9) DEFAULT NULL,
	   KEY `IX_NBR_TYPE` (`CELL_ID`,`RNC_ID`,`NBR_CELL_ID`,`NBR_RNC_ID`,`NBR_TYPE`)
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;    
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_rnc`
	(
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `RNC_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `MCC` varchar(3) DEFAULT NULL,
	  `MNC` varchar(2) DEFAULT NULL,
	  `TECH_MASK` tinyint(4) DEFAULT NULL,
	  `VENDOR` varchar(50) DEFAULT NULL,
	  `REGION` VARCHAR(50) DEFAULT NULL,
	  `SUB_REGION` VARCHAR(50) DEFAULT NULL,
	  `RNC_MODEL` VARCHAR(30) DEFAULT NULL,
	  `SITE_CNT` smallint(6) DEFAULT NULL,
	  `CELL_CNT` smallint(6) DEFAULT NULL,
	  `AVG_VORONOI` double DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_rnc_current`
	 (  
	  `NT_DATE` datetime DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `RNC_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `MCC` varchar(3) DEFAULT NULL,
	  `MNC` varchar(2) DEFAULT NULL,
	  `TECH_MASK` tinyint(4) DEFAULT NULL,
	  `VENDOR` varchar(50) DEFAULT NULL,
	  `REGION` VARCHAR(50) DEFAULT NULL,
	  `SUB_REGION` VARCHAR(50) DEFAULT NULL,
	  `RNC_MODEL` VARCHAR(30) DEFAULT NULL,
	  `SITE_CNT` smallint(6) DEFAULT NULL,
	  `CELL_CNT` smallint(6) DEFAULT NULL,
	  `AVG_VORONOI` double DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;        
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_cell_attribute`
	(
	`NT_DATE` DATETIME DEFAULT NULL,
	`RNC_ID` int(11) DEFAULT NULL,
	`CELL_ID` MEDIUMINT(9) DEFAULT NULL,
	`CELL_NAME` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	`ATTRIBUTE_NAME` VARCHAR(256) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	`ATTRIBUTIE_VALUE` TEXT DEFAULT NULL,
	`TYPE` VARCHAR(100) DEFAULT NULL,
	`VENDOR` VARCHAR(50) DEFAULT NULL,
	`P_VALID` TINYINT(4) DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_cell_attribute_current`
	(
	`NT_DATE` DATETIME DEFAULT NULL,
	`RNC_ID` int(11) DEFAULT NULL,
	`CELL_ID` MEDIUMINT(9) DEFAULT NULL,
	`CELL_NAME` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	`ATTRIBUTE_NAME` VARCHAR(256) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	`ATTRIBUTIE_VALUE` TEXT DEFAULT NULL,
	`TYPE` VARCHAR(100) DEFAULT NULL,
	`VENDOR` VARCHAR(50) DEFAULT NULL,
	`P_VALID` TINYINT(4) DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_sc_code` 
	(
	  `NT_DATE` DATETIME DEFAULT NULL,
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` MEDIUMINT(9) DEFAULT NULL,
	  `CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LONGITUDE` DOUBLE DEFAULT NULL,
	  `LATITUDE` DOUBLE DEFAULT NULL,
	  `SCRAMBLING_CODE` SMALLINT(6) DEFAULT NULL,
	  `DL_UARFCN` mediumint(9) DEFAULT NULL,
	  `LAC` mediumint(9) unsigned  DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_neighbor_voronoi`
	 (
	  `RNC_ID` int(11) DEFAULT NULL,
	  `SITE_ID` VARCHAR(20) DEFAULT NULL,
	  `NBR_RNC_ID` int(11) DEFAULT NULL,
	  `NBR_SITE_ID` VARCHAR(20) DEFAULT NULL,
	  `ANGLE` DOUBLE DEFAULT NULL,
	  `DISTANCE` DOUBLE DEFAULT NULL, 
	  `INDOOR_TYPE` TINYINT(4) DEFAULT NULL,
	  `SITEDENSITY_RANGE` INT(11) DEFAULT NULL,
	  `REFINE_DISTANCE` FLOAT DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1
	;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_neighbor_interfreq` (
	  `nt_date` date DEFAULT NULL,
	  `rnc_id` int(11) DEFAULT NULL,
	  `cell_id` mediumint(9) DEFAULT NULL,
	  `nbr_rnc_id` mediumint(9) DEFAULT NULL,
	  `nbr_cell_id` mediumint(9) DEFAULT NULL,
	  `angle` double DEFAULT NULL,
	  `distance` double DEFAULT NULL
	) ENGINE=MyISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_related` (
	  `source_rnc_id` MEDIUMINT(9) DEFAULT NULL,
	  `source_cell_id` MEDIUMINT(9) DEFAULT NULL,
	  `source_site_id` VARCHAR(20) DEFAULT NULL,
	  `target_rnc_id` MEDIUMINT(9) DEFAULT NULL,
	  `target_cell_id` MEDIUMINT(9) DEFAULT NULL,
	  `target_site_id` VARCHAR(20) DEFAULT NULL,
	  `UARFCN` mediumint(9) DEFAULT NULL,
	  `dist` DOUBLE DEFAULT NULL,
	  `angle` MEDIUMINT(9) DEFAULT NULL,
	  `nbr_type` TINYINT(1) DEFAULT NULL,
	  `voronoi` TINYINT(1) DEFAULT NULL,
	  `threshold` FLOAT DEFAULT NULL
	) ENGINE=MyISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`opt_sco_result` (
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` int(11) DEFAULT NULL,
	  `SOURCE_CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `LONGITUDE` double DEFAULT NULL,
	  `LATITUDE` double DEFAULT NULL,
	  `PSC` smallint(6) DEFAULT NULL,
	  `NBR_RNC_ID` int(11) DEFAULT NULL,
	  `NBR_CELL_ID` int(11) DEFAULT NULL,
	  `TARGET_CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	  `DISTANCE` double DEFAULT NULL,
	  `DEPTH` smallint(6) DEFAULT NULL,
	  `DIS_TILE_CNT` mediumint(9) DEFAULT ''0'',
	  `UNI_TILE_CNT` mediumint(9) DEFAULT ''0'',
	  `DEPTH_1_CELL` int(11) DEFAULT NULL,
	  `DEPTH_1_RNC` int(11) DEFAULT NULL,
	  `DEPTH_2_CELL` int(11) DEFAULT NULL,
	  `DEPTH_2_RNC` int(11) DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;  
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`opt_sco_suggest` (
	  `RNC_ID` int(11) DEFAULT NULL,
	  `CELL_ID` int(11) DEFAULT NULL,
	  `NBR_RNC_ID` int(11) DEFAULT NULL,
	  `NBR_CELL_ID` int(11) DEFAULT NULL,
	  `PSC_SUGGEST` smallint(6) DEFAULT NULL,
	  `DISTANCE` double DEFAULT NULL,
	  `PSC_DEPTH` smallint(6) DEFAULT NULL,
	  `PSC_RNC_ID` varchar(64) DEFAULT NULL,
	  `PSC_CELL_ID` varchar(64) DEFAULT NULL,
	  `SUGGEST_ORDER` int(11) DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt; 
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_repeater_current` (
	`NT_DATE` DATETIME DEFAULT NULL,
	`RNC_ID` int(11) DEFAULT NULL,
	`CELL_ID` int(11) DEFAULT NULL,
	`CELL_NAME` varchar(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
	`LONGITUDE` double DEFAULT NULL,
	`LATITUDE` double DEFAULT NULL,
	`AZIMUTH` SMALLINT(6) DEFAULT NULL,
	`ANTENNA_MODEL` varchar(50) DEFAULT NULL,
	`ANTENNA_TYPE` tinyint(4) DEFAULT NULL,
	`HEIGHT` double DEFAULT NULL,
	`BEAMWIDTH_H` double DEFAULT NULL,
	`BEAMWIDTH_V` double DEFAULT NULL,
	`CPICH_POWER` double DEFAULT NULL,
	`EIRP` double DEFAULT NULL,
	`ANTENNA_GAIN` double DEFAULT NULL,
	`FEEDER_LOSS` double DEFAULT NULL,
	`DOWNTILT_EL` double DEFAULT NULL,
	`DOWNTILT_MEC` double DEFAULT NULL,
	`ELEVATION` double DEFAULT NULL,
	`REPEATER_TYPE` VARCHAR(20) DEFAULT NULL,
	`REPEATER_PG_DELAY` VARCHAR(20) DEFAULT NULL, 
	`INDOOR_TYPE` TINYINT(4) DEFAULT NULL,
	`ANTENNA_ID` TINYINT(4) DEFAULT ''1''
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`rule_nt_nbr_umts` (
	`vendorID` VARCHAR(20) DEFAULT NULL,
	`sourceFreq` SMALLINT(6) DEFAULT NULL,
	`sourceUarfcn` MEDIUMINT(9) DEFAULT NULL,
	`targetFreq` SMALLINT(6) DEFAULT NULL,
	`targetUarfcn` MEDIUMINT(9) DEFAULT NULL,
	`coSector` SMALLINT(6) DEFAULT NULL,
	`nbrtype` TINYINT(4) DEFAULT NULL,
	`allowed` TINYINT(4) DEFAULT NULL,
	`minNbrCnt` SMALLINT(6) DEFAULT NULL,
	`maxNbrCnt` SMALLINT(6) DEFAULT NULL,
	`supportFlag` TINYINT(4) DEFAULT NULL,
	`Border_NbrCnt` SMALLINT(6) DEFAULT NULL,
	`SIB` TINYINT(4) DEFAULT NULL
	) ENGINE=MYISAM DEFAULT CHARSET=utf8 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`NT_DROPPED_CELL` (
		`TAC` MEDIUMINT(9) ,
		`ENODEB_ID` MEDIUMINT(9) ,
		`CELL_ID` MEDIUMINT(9) ,
		`LAC` MEDIUMINT(9) ,
		`TECHNOLOGY` CHAR(1),
		`VENDOR` VARCHAR(20) CHARACTER SET UTF8
		)ENGINE=MYISAM DEFAULT CHARSET=LATIN1 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`NT_RNCLIST` (
		`GTGW` VARCHAR(20) CHARACTER SET UTF8,
		`RNC` SMALLINT(6),
		`RNC_NAME` VARCHAR(20) CHARACTER SET UTF8,
		`CELL COUNT` MEDIUMINT(9) ,
		`STATUS` VARCHAR(50) CHARACTER SET UTF8
		)ENGINE=MYISAM DEFAULT CHARSET=LATIN1 DELAY_KEY_WRITE=1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	
  
	IF extarTables='true' THEN
	    SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`nt_extra` (
		  NT_DATE DATETIME DEFAULT NULL,
		  RNC_ID MEDIUMINT DEFAULT NULL,
		  SITE_ID VARCHAR(20) DEFAULT NULL,
		  SITE_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
		  CELL_ID MEDIUMINT DEFAULT NULL,
