CREATE DEFINER=`covmo`@`%` PROCEDURE `SP_CreateDB_Schema_GSM`(IN GT_DB VARCHAR(50),IN FILEDATE VARCHAR(18))
BEGIN
	DECLARE FILE_ENDTIME VARCHAR(20) DEFAULT CONCAT(LEFT(FILEDATE,4),'-',SUBSTRING(FILEDATE,5,2),'-',SUBSTRING(FILEDATE,7,2),' ', SUBSTRING(FILEDATE,15,2),':',SUBSTRING(FILEDATE,17,2),':00');	
	DECLARE DAILY_JUDGE  BOOLEAN DEFAULT IF(RIGHT(FILEDATE,9)='0000_0000',TRUE,FALSE);
	DECLARE PARTITION_STR VARCHAR(1500) DEFAULT 
		' PARTITION BY LIST (DATA_HOUR)
		(
		 PARTITION h0 VALUES IN (0) ENGINE = MYISAM,
		 PARTITION h1 VALUES IN (1) ENGINE = MYISAM,
		 PARTITION h2 VALUES IN (2) ENGINE = MYISAM,
		 PARTITION h3 VALUES IN (3) ENGINE = MYISAM,
		 PARTITION h4 VALUES IN (4) ENGINE = MYISAM,
		 PARTITION h5 VALUES IN (5) ENGINE = MYISAM,
		 PARTITION h6 VALUES IN (6) ENGINE = MYISAM,
		 PARTITION h7 VALUES IN (7) ENGINE = MYISAM,
		 PARTITION h8 VALUES IN (8) ENGINE = MYISAM,
		 PARTITION h9 VALUES IN (9) ENGINE = MYISAM,
		 PARTITION h10 VALUES IN (10) ENGINE = MYISAM,
		 PARTITION h11 VALUES IN (11) ENGINE = MYISAM,
		 PARTITION h12 VALUES IN (12) ENGINE = MYISAM,
		 PARTITION h13 VALUES IN (13) ENGINE = MYISAM,
		 PARTITION h14 VALUES IN (14) ENGINE = MYISAM,
		 PARTITION h15 VALUES IN (15) ENGINE = MYISAM,
		 PARTITION h16 VALUES IN (16) ENGINE = MYISAM,
		 PARTITION h17 VALUES IN (17) ENGINE = MYISAM,
		 PARTITION h18 VALUES IN (18) ENGINE = MYISAM,
		 PARTITION h19 VALUES IN (19) ENGINE = MYISAM,
		 PARTITION h20 VALUES IN (20) ENGINE = MYISAM,
		 PARTITION h21 VALUES IN (21) ENGINE = MYISAM,
		 PARTITION h22 VALUES IN (22) ENGINE = MYISAM,
		 PARTITION h23 VALUES IN (23) ENGINE = MYISAM	
		 );';
	DECLARE TABLE_OPT_RELATION_COL_STR VARCHAR(2000) DEFAULT 
		'
		`DATA_DATE` DATETIME DEFAULT NULL,
		`DATA_HOUR` TINYINT(4) DEFAULT NULL,
		`SERVING_BSC` MEDIUMINT(9) NOT NULL,
		`SERVING_LAC` MEDIUMINT(9) NOT NULL,
		`SERVING_CID` MEDIUMINT(9) NOT NULL,
		`SERVING_CELL_NAME` VARCHAR(150) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
		`SITE_ID` VARCHAR(60) CHARACTER SET UTF8 DEFAULT NULL,
		`TARGET_BSC` MEDIUMINT(9) NOT NULL,
		`TARGET_LAC` MEDIUMINT(9) NOT NULL,
		`TARGET_CID` MEDIUMINT(9) NOT NULL,
		`TARGET_CELL_NAME` VARCHAR(150) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
		`TARGET_SITE_ID` VARCHAR(60) DEFAULT NULL,
		`EVENT_TYPE` TINYINT(4) DEFAULT NULL,
		`NBR_TYPE` VARCHAR(30) DEFAULT NULL,
		`LONGITUDE` DOUBLE DEFAULT NULL,
		`LATITUDE` DOUBLE DEFAULT NULL,
		`DISTANCE_METER` DOUBLE DEFAULT NULL,
		`HO_ALL_CNT` SMALLINT(6) DEFAULT NULL,
		`HO_SUCC_CNT` SMALLINT(6) DEFAULT NULL,
		`HO_FAIL_CNT` SMALLINT(6) DEFAULT NULL,
		`HO_DROP_CNT` SMALLINT(6) DEFAULT NULL,
		`MEAS_RXLEV` DOUBLE DEFAULT NULL,
		`MEAS_CNT` SMALLINT(6) DEFAULT NULL,
		`SERV_RXLEV` DOUBLE DEFAULT NULL,
		`SERV_RXLEV_CNT` SMALLINT(6) DEFAULT NULL,
		`SERV_RXQUAL` DOUBLE DEFAULT NULL,
		`SERV_RXQUAL_CNT` SMALLINT(6) DEFAULT NULL
	';
	
	DECLARE TABLE_CALL_COL_STR VARCHAR(8000) DEFAULT 
		'
		`CALL_ID` BIGINT(20) UNSIGNED DEFAULT NULL,
		`START_TIME` DATETIME DEFAULT NULL,
		`START_TIME_MS` MEDIUMINT(9) DEFAULT NULL,
		`END_TIME` DATETIME DEFAULT NULL,
		`END_TIME_MS` MEDIUMINT(9) DEFAULT NULL,
		`DURATION` BIGINT(20) DEFAULT NULL,
		`IMSI` VARCHAR(20) DEFAULT NULL,
		`TMSI` VARCHAR(20) DEFAULT NULL,
		`IMEI` VARCHAR(20) DEFAULT NULL,
		`START_LAC` MEDIUMINT(9) UNSIGNED DEFAULT NULL,
		`START_CELL_ID` MEDIUMINT(9) DEFAULT NULL,
		`START_BSC_ID` MEDIUMINT(9) DEFAULT NULL,
		`START_TCH_TYPE` SMALLINT(5) DEFAULT NULL,
		`END_LAC` MEDIUMINT(9) UNSIGNED DEFAULT NULL,
		`END_CELL_ID` MEDIUMINT(9) DEFAULT NULL,
		`END_BSC_ID` MEDIUMINT(9) DEFAULT NULL,
		`END_TCH_TYPE` SMALLINT(5) DEFAULT NULL,
		`CALL_TYPE` SMALLINT(6) DEFAULT NULL,
		`BSSMAP_FAILURE_CNT` BIGINT(20) DEFAULT NULL,
		`BSSMAP_FAILURE_EVENT_LAST` MEDIUMINT(9) DEFAULT NULL,
		`BSSMAP_FAILURE_CAUSE_LAST` BIGINT(20) DEFAULT NULL,
		`RSL_FAILURE_CNT` MEDIUMINT(9) DEFAULT NULL,
		`RSL_FAILURE_EVENT_LAST` BIGINT(20) DEFAULT NULL,
		`RSL_FAILURE_CAUSE_LAST` MEDIUMINT(9) DEFAULT NULL,
		`RR_DTAP_FAILURE_CNT` BIGINT(20) DEFAULT NULL,
		`RR_DTAP_FAILURE_EVENT_LAST` MEDIUMINT(9) DEFAULT NULL,
		`RR_DTAP_FAILURE_CAUSE_LAST` BIGINT(20) DEFAULT NULL,	
		`MCC` CHAR(3) DEFAULT NULL,
		`MNC` VARCHAR(3) DEFAULT NULL,
		`CALL_STATUS` SMALLINT(6) DEFAULT NULL,
		`REQUEST_TYPE` SMALLINT(6) DEFAULT NULL,
		`HO_ATTEMPT_COUNT` SMALLINT(6) DEFAULT NULL,
		`HO_FAILURE_COUNT` SMALLINT(6) DEFAULT NULL,
		`ACCESS_POINT_NAME` VARCHAR(50) DEFAULT NULL,
		`CALL_SETUP_TIME` MEDIUMINT(9) DEFAULT NULL,
		`B_PARTY_NUMBER` VARCHAR(15) DEFAULT NULL,
		`ACCESS_REASON` BIGINT(20) DEFAULT NULL,
		`ACCESS_RESULT_CAUSE` BIGINT(20) DEFAULT NULL,
		`RELEASE_RESULT_CAUSE` BIGINT(20) DEFAULT NULL,
		`APN` SMALLINT(6) DEFAULT NULL,
		`RR_CONN_CAUSE` TINYINT(4) DEFAULT NULL,
		`IMSI_GROUP_ID` SMALLINT(6) DEFAULT NULL,
		`DATA_DATE` DATE DEFAULT NULL,
		`DATA_HOUR` TINYINT(4) DEFAULT NULL,
		`MOVING` TINYINT(4) DEFAULT NULL,
		`INDOOR` TINYINT(4) DEFAULT NULL,
		`MOVING_SPEED` FLOAT DEFAULT NULL,
		`MOVING_TYPE` TINYINT(4) DEFAULT NULL,
		`POS_FIRST_BSC` MEDIUMINT(9) DEFAULT NULL,
		`POS_FIRST_LAC` MEDIUMINT(9) UNSIGNED DEFAULT NULL,
		`POS_FIRST_CELL_INDOOR` TINYINT(4) DEFAULT NULL,
		`POS_FIRST_SITE` VARCHAR(20) DEFAULT NULL,
		`POS_FIRST_CELL` MEDIUMINT(9) DEFAULT NULL,
		`POS_FIRST_CELL_NAME` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
		`POS_FIRST_LOC` BIGINT(20) DEFAULT NULL,
		`POS_FIRST_BANDINDEX` VARCHAR(10) DEFAULT NULL,
		`POS_FIRST_BCCH_ARFCN` SMALLINT(5) UNSIGNED DEFAULT NULL,
		`POS_FIRST_RXLEV_FULL_DOWNLINK` FLOAT DEFAULT NULL,
		`POS_FIRST_RXQUAL_FULL_DOWNLINK` FLOAT DEFAULT NULL,
		`POS_FIRST_RXLEV_FULL_UPLINK` FLOAT DEFAULT NULL,
		`POS_FIRST_RXQUAL_FULL_UPLINK` FLOAT DEFAULT NULL,
		`POS_LAST_BSC` MEDIUMINT(9) DEFAULT NULL,
		`POS_LAST_LAC` MEDIUMINT(9) UNSIGNED DEFAULT NULL,
		`POS_LAST_CELL_INDOOR` TINYINT(4) DEFAULT NULL,
		`POS_LAST_SITE` VARCHAR(20) DEFAULT NULL,
		`POS_LAST_CELL` MEDIUMINT(9) DEFAULT NULL,
		`POS_LAST_CELL_NAME` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
		`POS_LAST_LOC` BIGINT(20) DEFAULT NULL,
		`POS_LAST_BANDINDEX` VARCHAR(10) DEFAULT NULL,
		`POS_LAST_BCCH_ARFCN` SMALLINT(5) UNSIGNED DEFAULT NULL,
		`POS_LAST_RXLEV_FULL_DOWNLINK` FLOAT DEFAULT NULL,
		`POS_LAST_RXQUAL_FULL_DOWNLINK` FLOAT DEFAULT NULL,
		`POS_LAST_RXLEV_FULL_UPLINK` FLOAT DEFAULT NULL,
		`POS_LAST_RXQUAL_FULL_UPLINK` FLOAT DEFAULT NULL,
		`MAKE_ID` SMALLINT(6) DEFAULT NULL,
		`MODEL_ID` MEDIUMINT(9) DEFAULT NULL,
		`IRAT_HO_STATUS` TINYINT(4) DEFAULT NULL,
		`BATCH` SMALLINT(6) DEFAULT NULL,
		`POS_FIRST_LON` FLOAT DEFAULT NULL,
		`POS_FIRST_LAT`FLOAT DEFAULT NULL,
		`POS_LAST_LON` FLOAT DEFAULT NULL,
		`POS_LAST_LAT` FLOAT DEFAULT NULL,
		`POS_FIRST_CONFIDENCE` TINYINT(4) DEFAULT NULL,
		`POS_LAST_CONFIDENCE` TINYINT(4) DEFAULT NULL
	';
	
	DECLARE TABLE_TILE_CELL_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		DATA_HOUR TINYINT(4) NOT NULL,
		INDOOR TINYINT(4) NOT NULL,
		MOVING TINYINT(4) NOT NULL,
		TILE_ID BIGINT(20) NOT NULL,
		BSC_ID MEDIUMINT(9) NOT NULL,
		CELL_ID MEDIUMINT(9) NOT NULL,
		CALL_TYPE TINYINT(4) NOT NULL,
		CALL_STATUS TINYINT(4) NOT NULL,
		BANDINDEX SMALLINT(6) DEFAULT NULL,
		CELL_INDOOR TINYINT(4) DEFAULT NULL,
		CLUSTER_ID MEDIUMINT(9) DEFAULT NULL,
		LAC MEDIUMINT(9) DEFAULT NULL,
		SITE_ID VARCHAR(20) CHARACTER SET UTF8 DEFAULT NULL,
		CELL_LON DOUBLE DEFAULT NULL,
		CELL_LAT DOUBLE DEFAULT NULL,
		BCCH_ARFCN MEDIUMINT(9) DEFAULT NULL,
		CELL_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL
	';
	DECLARE TABLE_CELL_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		DATA_HOUR TINYINT(4) NOT NULL,
		INDOOR TINYINT(4) NOT NULL,
		MOVING TINYINT(4) NOT NULL,
		BSC_ID MEDIUMINT(9) NOT NULL,
		CELL_ID MEDIUMINT(9) NOT NULL,
		CALL_TYPE TINYINT(4) NOT NULL,
		CALL_STATUS TINYINT(4) NOT NULL,
		BANDINDEX SMALLINT(6) DEFAULT NULL,
		CELL_INDOOR TINYINT(4) DEFAULT NULL,
		CLUSTER_ID MEDIUMINT(9) DEFAULT NULL,
		LAC MEDIUMINT(9) DEFAULT NULL,
		SITE_ID VARCHAR(20) CHARACTER SET UTF8 DEFAULT NULL,
		CELL_LON DOUBLE DEFAULT NULL,
		CELL_LAT DOUBLE DEFAULT NULL,
		BCCH_ARFCN MEDIUMINT(9) DEFAULT NULL,
		CELL_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL
	';
	DECLARE TABLE_TILE_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		DATA_HOUR TINYINT(4) NOT NULL,
		INDOOR TINYINT(4) NOT NULL,
		MOVING TINYINT(4) NOT NULL,
		TILE_ID BIGINT(20) NOT NULL,
		CALL_TYPE TINYINT(4) NOT NULL,
		CALL_STATUS TINYINT(4) NOT NULL
	';
	
	DECLARE TABLE_CELL_DEF_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		DATA_HOUR TINYINT(4) NOT NULL,
		BSC_ID MEDIUMINT(9) NOT NULL,
		CELL_ID MEDIUMINT(9) NOT NULL,
		CELL_LON DOUBLE DEFAULT NULL,
		CELL_LAT DOUBLE DEFAULT NULL,
		CELL_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL
	';
	
	DECLARE TABLE_TILE_DEF_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		DATA_HOUR TINYINT(4) NOT NULL,
		TILE_ID BIGINT(20) NOT NULL
	';
	DECLARE TABLE_TILE_CELL_DY_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		INDOOR TINYINT(4) NOT NULL,
		MOVING TINYINT(4) NOT NULL,
		TILE_ID BIGINT(20) NOT NULL,
		BSC_ID MEDIUMINT(9) NOT NULL,
		CELL_ID MEDIUMINT(9) NOT NULL,
		CALL_TYPE TINYINT(4) NOT NULL,
		CALL_STATUS TINYINT(4) NOT NULL,
		BANDINDEX SMALLINT(6) DEFAULT NULL,
		CELL_INDOOR TINYINT(4) DEFAULT NULL,
		CLUSTER_ID MEDIUMINT(9) DEFAULT NULL,
		LAC MEDIUMINT(9) DEFAULT NULL,
		SITE_ID VARCHAR(20) CHARACTER SET UTF8 DEFAULT NULL,
		CELL_LON DOUBLE DEFAULT NULL,
		CELL_LAT DOUBLE DEFAULT NULL,
		BCCH_ARFCN MEDIUMINT(9) DEFAULT NULL,
		CELL_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL
	';
	
	DECLARE TABLE_TILE_CELL_DY_KEY_STR VARCHAR(1000) DEFAULT 
		'
		PRIMARY KEY (
		DATA_DATE
		,INDOOR
		,MOVING
		,TILE_ID
		,BSC_ID
		,CELL_ID
		,CALL_TYPE
		,CALL_STATUS	
	)
	';
	
	DECLARE TABLE_CELL_DY_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		INDOOR TINYINT(4) NOT NULL,
		MOVING TINYINT(4) NOT NULL,
		BSC_ID MEDIUMINT(9) NOT NULL,
		CELL_ID MEDIUMINT(9) NOT NULL,
		CALL_TYPE TINYINT(4) NOT NULL,
		CALL_STATUS TINYINT(4) NOT NULL,
		BANDINDEX SMALLINT(6) DEFAULT NULL,
		CELL_INDOOR TINYINT(4) DEFAULT NULL,
		CLUSTER_ID MEDIUMINT(9) DEFAULT NULL,
		LAC MEDIUMINT(9) DEFAULT NULL,
		SITE_ID VARCHAR(20) CHARACTER SET UTF8 DEFAULT NULL,
		CELL_LON DOUBLE DEFAULT NULL,
		CELL_LAT DOUBLE DEFAULT NULL,
		BCCH_ARFCN MEDIUMINT(9) DEFAULT NULL,
		CELL_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL
	';
	DECLARE TABLE_CELL_DY_KEY_STR VARCHAR(1000) DEFAULT 
		'
		PRIMARY KEY (
		DATA_DATE
		,INDOOR
		,MOVING
		,BSC_ID
		,CELL_ID
		,CALL_TYPE
		,CALL_STATUS
	)
	';
	DECLARE TABLE_TILE_DY_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		INDOOR TINYINT(4) NOT NULL,
		MOVING TINYINT(4) NOT NULL,
		TILE_ID BIGINT(20) NOT NULL,
		CALL_TYPE TINYINT(4) NOT NULL,
		CALL_STATUS TINYINT(4) NOT NULL
	';
	DECLARE TABLE_TILE_DY_KEY_STR VARCHAR(1000) DEFAULT 
		'
		PRIMARY KEY (
		DATA_DATE
		,INDOOR
		,MOVING
		,TILE_ID
		,CALL_TYPE
		,CALL_STATUS
	)
	';
	
	DECLARE TABLE_CELL_DEF_DY_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		BSC_ID MEDIUMINT(9) NOT NULL,
		CELL_ID MEDIUMINT(9) NOT NULL,
		CELL_LON DOUBLE DEFAULT NULL,
		CELL_LAT DOUBLE DEFAULT NULL,
		CELL_NAME VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL
	';
	DECLARE TABLE_CELL_DEF_DY_KEY_STR VARCHAR(1000) DEFAULT 
		'
		PRIMARY KEY (
		DATA_DATE
		,BSC_ID
		,CELL_ID
	)
	';
	
	DECLARE TABLE_TILE_DEF_DY_KEY_COL_STR VARCHAR(1000) DEFAULT 
		'
		DATA_DATE DATE NOT NULL,
		TILE_ID BIGINT(20) NOT NULL
	';
	DECLARE TABLE_TILE_DEF_DY_KEY_STR VARCHAR(1000) DEFAULT 
		'
		PRIMARY KEY (
		DATA_DATE
		, TILE_ID
	)
	';
	DECLARE START_COL_STR VARCHAR(1000) DEFAULT 
		'
		CALL_CNT MEDIUMINT(9) DEFAULT 0,
		RXLEV_FULL_DOWNLINK_CNT MEDIUMINT(9) DEFAULT NULL,
		RXLEV_FULL_DOWNLINK_SUM FLOAT DEFAULT NULL,
		RXQUAL_FULL_DOWNLINK_CNT MEDIUMINT(9) DEFAULT NULL,
		RXQUAL_FULL_DOWNLINK_SUM FLOAT DEFAULT NULL,
		RXLEV_FULL_DOWNLINK_AVG DOUBLE DEFAULT NULL,
		RXQUAL_FULL_DOWNLINK_AVG DOUBLE DEFAULT NULL,
		IMSI_CNT INT(11) DEFAULT NULL,
		CALL_TYPE_10 MEDIUMINT(9) DEFAULT NULL,
		CALL_TYPE_16 MEDIUMINT(9) DEFAULT NULL,
		CALL_TYPE_20 MEDIUMINT(9) DEFAULT NULL,
		CALL_TYPE_99 MEDIUMINT(9) DEFAULT NULL,
		RXLEV_FULL_UPLINK_SUM DOUBLE DEFAULT NULL,
		RXQUAL_FULL_UPLINK_SUM DOUBLE DEFAULT NULL,
		RXLEV_FULL_UPLINK_CNT INT(11) DEFAULT NULL,
		RXQUAL_FULL_UPLINK_CNT INT(11) DEFAULT NULL,
		RXLEV_FULL_UPLINK_MED_AVG FLOAT DEFAULT NULL,
		RXQUAL_FULL_UPLINK_MED_AVG FLOAT DEFAULT NULL
	';
	DECLARE END_COL_STR VARCHAR(1500) DEFAULT 
		'
		CALL_CNT MEDIUMINT(9) DEFAULT 0,
		RXLEV_FULL_DOWNLINK_CNT MEDIUMINT(9) DEFAULT NULL,
		RXLEV_FULL_DOWNLINK_SUM FLOAT DEFAULT NULL,
		RXQUAL_FULL_DOWNLINK_CNT MEDIUMINT(9) DEFAULT NULL,
		RXQUAL_FULL_DOWNLINK_SUM FLOAT DEFAULT NULL,
		RXLEV_FULL_UPLINK_SUM DOUBLE DEFAULT NULL,
		RXQUAL_FULL_UPLINK_SUM DOUBLE DEFAULT NULL,
		RXLEV_FULL_UPLINK_CNT INT(11) DEFAULT NULL,
		RXQUAL_FULL_UPLINK_CNT INT(11) DEFAULT NULL,
		RXLEV_FULL_UPLINK_MED_AVG FLOAT DEFAULT NULL,
		RXQUAL_FULL_UPLINK_MED_AVG FLOAT DEFAULT NULL,
		BSSMAP_11 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_12 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_13 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_14 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_15 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_16 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_17 MEDIUMINT(9) DEFAULT NULL,
		BSSMAP_18 MEDIUMINT(9) DEFAULT NULL,
		RSL_21 MEDIUMINT(9) DEFAULT NULL,
		RSL_22 MEDIUMINT(9) DEFAULT NULL,
		RSL_23 MEDIUMINT(9) DEFAULT NULL,
		RR_31 MEDIUMINT(9) DEFAULT NULL,
		RR_32 MEDIUMINT(9) DEFAULT NULL,
		RR_33 MEDIUMINT(9) DEFAULT NULL,
		RR_34 MEDIUMINT(9) DEFAULT NULL,
		RR_35 MEDIUMINT(9) DEFAULT NULL,
		RR_36 MEDIUMINT(9) DEFAULT NULL
	';
	DECLARE UE_COL_STR VARCHAR(1000) DEFAULT 
		'
		UE_CNT MEDIUMINT(9) DEFAULT 0
	';
	DECLARE FP_COL_STR VARCHAR(1600) DEFAULT 
		'
		FP_RXLEV_BEST_SUM DOUBLE DEFAULT NULL,
		FP_RXQUAL_BEST_SUM DOUBLE DEFAULT NULL,
		FP_RXLEV_BEST_MED_AVG DOUBLE DEFAULT NULL,
		FP_RXQUAL_BEST_MED_AVG DOUBLE DEFAULT NULL,
		FP_RXLEV_ALL_SUM DOUBLE DEFAULT NULL,
		FP_RXQUAL_ALL_SUM DOUBLE DEFAULT NULL,
		FP_RXLEV_ALL_MED_AVG DOUBLE DEFAULT NULL,
		FP_RXQUAL_ALL_MED_AVG DOUBLE DEFAULT NULL,
		MMR_CNT MEDIUMINT(9) DEFAULT NULL,
		BEST_CNT MEDIUMINT(9) DEFAULT NULL,
		BEST_SERVING_CNT MEDIUMINT(9) DEFAULT NULL,
		SERVING_CNT MEDIUMINT(9) DEFAULT NULL,
		RXLEV_GAP_SUM DOUBLE DEFAULT NULL,
		RXLEV_GAP_CNT MEDIUMINT(9) DEFAULT NULL,
		SERVING_RXLEV_BETTER_GAP_SUM DOUBLE DEFAULT NULL,
		SERVING_RXLEV_BETTER_GAP_CNT MEDIUMINT(9) DEFAULT NULL,
		SERVING_RXLEV_WORSE_GAP_SUM DOUBLE DEFAULT NULL,
		SERVING_RXLEV_WORSE_GAP_CNT MEDIUMINT(9) DEFAULT NULL,
		INTERFERED_CNT MEDIUMINT(9) DEFAULT NULL,
		MS_POWER_SUM MEDIUMINT(9) DEFAULT NULL,
		MS_POWER_CNT MEDIUMINT(9) DEFAULT NULL,
		TOTAL_SERVING_CNT MEDIUMINT(9) DEFAULT NULL,
		DURATION BIGINT(20) DEFAULT NULL,
		INTER_CELL_HO_CNT INT(11) DEFAULT NULL,
		INTER_CELL_HO_FAIL_CNT INT(11) DEFAULT NULL,
		INTRA_CELL_HO_CNT INT(11) DEFAULT NULL,
		INTRA_CELL_HO_FAIL_CNT INT(11) DEFAULT NULL
	';
	DECLARE CUSTOMER_USER_FLAG VARCHAR(50);
	SELECT LOWER(`value`) INTO CUSTOMER_USER_FLAG  FROM gt_gw_main.integration_param WHERE gt_group = 'sp' AND gt_name = 'special_imsi' ;
	
	SET @SqlCmd =CONCAT('CREATE DATABASE ', GT_DB);
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`table_call_gsm` (
				',TABLE_CALL_COL_STR,',
				KEY `CALL_ID` (`CALL_ID`),
				KEY `IMSI` (`IMSI`)
				',IF(DAILY_JUDGE=TRUE,'
				         ,KEY `IMEI` (`IMEI`),
					  KEY `IX_FIRST_CELL` (`POS_FIRST_CELL`),
					  KEY `IX_LAST_CELL` (`POS_LAST_CELL`)
				',''),'
				',IF(DAILY_JUDGE=FALSE,'
					 ,KEY `IX_POS_FIRST_BSC` (`POS_FIRST_BSC`,`POS_FIRST_CELL`),
					  KEY `IX_POS_LAST_BSC` (`POS_LAST_BSC`,`POS_LAST_CELL`),
					  KEY `IX_START_CELL` (`START_CELL_ID`)
				',''),'
				) ENGINE=MyISAM DEFAULT CHARSET=latin1 DELAY_KEY_WRITE=1'
				,IF(DAILY_JUDGE=TRUE,PARTITION_STR,';'));
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`table_call_nopos_gsm` (
				',TABLE_CALL_COL_STR,'
				',IF(DAILY_JUDGE=TRUE,'
				         ,KEY `CALL_ID` (`CALL_ID`),
					  KEY `IMSI` (`IMSI`),
					  KEY `IMEI` (`IMEI`),
					  KEY `IX_FIRST_CELL` (`POS_FIRST_CELL`),
					  KEY `IX_LAST_CELL` (`POS_LAST_CELL`)
				',''),'
				) ENGINE=MyISAM DEFAULT CHARSET=latin1 DELAY_KEY_WRITE=1'
				,IF(DAILY_JUDGE=TRUE,PARTITION_STR,';'));
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`table_call_gsm_dump` (
				',TABLE_CALL_COL_STR,'
				) ENGINE=MyISAM DEFAULT CHARSET=latin1 DELAY_KEY_WRITE=1;
				');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	SET @SqlCmd=CONCAT('CREATE TABLE ',GT_DB,'.`table_position_gsm` (
				  `CALL_ID` BIGINT(20) UNSIGNED DEFAULT NULL,
				  `DATE_TIME` DATETIME DEFAULT NULL,
				  `DATE_TIME_MS` MEDIUMINT(9) DEFAULT NULL,
				  `SEQ_ID` MEDIUMINT(9) DEFAULT NULL,
				  `EVENT_TYPE` TINYINT(4) DEFAULT NULL,
				  `SERVING_BSC` MEDIUMINT(9) DEFAULT NULL,
				  `SERVING_LAC` MEDIUMINT(9) UNSIGNED DEFAULT NULL,
				  `SERVING_CID` MEDIUMINT(9) DEFAULT NULL,
				  `SERVING_CELL_NAME` VARCHAR(50) CHARACTER SET utf8 COLLATE utf8_swedish_ci DEFAULT NULL,
				  `SERVING_SITE_ID` VARCHAR(20)  DEFAULT NULL,
				  `SERVING_BCCH_ARFCN` SMALLINT(6) UNSIGNED  DEFAULT NULL,
				  `SERVING_BANDINDEX` VARCHAR(10)  DEFAULT NULL,
				  `RXLEV_FULL_UPLINK` FLOAT DEFAULT NULL,
				  `RXLEV_SUB_UPLINK` FLOAT DEFAULT NULL,
