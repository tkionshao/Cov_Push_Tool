CREATE DEFINER=`covmo`@`%` PROCEDURE `SP_Sub_Generate_Dominant_GSM`(IN GT_DB VARCHAR(100), IN KIND VARCHAR(20), IN VENDOR_SOURCE VARCHAR(20),IN GT_COVMO VARCHAR(100))
BEGIN
	DECLARE BSC_ID INT;
	DECLARE O_GT_DB VARCHAR(100) DEFAULT GT_DB;
	DECLARE START_TIME DATETIME DEFAULT SYSDATE();
	DECLARE PARTITION_ID INT DEFAULT SUBSTRING(RIGHT(GT_DB,18),10,2) ;
	DECLARE SH_EH VARCHAR(9) DEFAULT RIGHT(GT_DB,9);
	DECLARE GT_DATE VARCHAR(18) DEFAULT RIGHT(GT_DB,18);
	DECLARE STARTHOUR VARCHAR(2) DEFAULT SUBSTRING(RIGHT(GT_DB,18),10,2);
	DECLARE ENDHOUR VARCHAR(2) DEFAULT IF(SUBSTRING(RIGHT(GT_DB,18),15,2)='00','24',SUBSTRING(RIGHT(GT_DB,18),15,2));
	DECLARE RUN VARCHAR(20);
	DECLARE WORKER_ID VARCHAR(10) DEFAULT CONNECTION_ID();
	DECLARE CURRENT_NT_DB VARCHAR(50) DEFAULT CONCAT('gt_nt_',gt_strtok(GT_DB,3,'_'));
	SET SESSION group_concat_max_len = 100000;
	SET SESSION max_heap_table_size=4*1024*1024*1024;
	SET SESSION tmp_table_size=4*1024*1024*1024;
	SET SESSION sort_buffer_size = 1024*1024*1024; 
	SET SESSION read_buffer_size=1024*1024*1024;
	
	SELECT RIGHT(gt_strtok(GT_DB,2,'_'),3) INTO BSC_ID;
	
	SELECT REPLACE(GT_DB,SH_EH,'0000_0000') INTO GT_DB;
	
	IF VENDOR_SOURCE = 'GW' THEN
		IF KIND = 'DAILY' THEN
			SET RUN = '_tmp';
		ELSEIF KIND = 'RERUN' THEN
			SET RUN = '_rerun';
		END IF;
	ELSEIF VENDOR_SOURCE = 'AP' THEN
		SET RUN = '';
	END IF;
	SET @SqlCmd=CONCAT('ALTER TABLE ',GT_DB,RUN,'.table_tile_dominant_cell_gsm TRUNCATE PARTITION h',PARTITION_ID,';');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	INSERT INTO gt_gw_main.sp_log VALUES(O_GT_DB,'SP_Sub_Generate_Start_GSM','CREATE temp TABLE tmp_table_tile_dominant_cell_gsm ', NOW());
	
	SET @SqlCmd=CONCAT('DROP TEMPORARY TABLE  IF EXISTS ',GT_DB,RUN,'.tmp_table_tile_dominant_gsm_',WORKER_ID,';');
	PREPARE stmt FROM @sqlcmd;
	EXECUTE stmt;
	DEALLOCATE PREPARE stmt;
	
	SET @SqlCmd=CONCAT('CREATE TEMPORARY TABLE ',GT_DB,RUN,'.tmp_table_tile_dominant_gsm_',WORKER_ID,' 
			SELECT * FROM ',GT_DB,RUN,'.table_tile_dominant_cell_gsm WHERE 1<>1;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	INSERT INTO gt_gw_main.sp_log VALUES(O_GT_DB,'SP_Sub_Generate_Start_GSM','INSERT INTO tmp_table_tile_dominant_cell_gsm ', NOW());
	
	SET @SqlCmd=CONCAT('INSERT INTO  ',GT_DB,RUN,'.tmp_table_tile_dominant_gsm_',WORKER_ID,'
				(
				DATA_DATE,
				DATA_HOUR,
				INDOOR,
				MOVING,
				TILE_ID,
				BSC_ID,
				CELL_ID,
				BANDINDEX,
				CELL_INDOOR,
				CLUSTER_ID,
				LAC,
				SITE_ID,
				CELL_LON,
				CELL_LAT,
				BCCH_ARFCN,
				CELL_NAME,
				CALL_CNT,
				RXLEV_FULL_DOWNLINK_CNT,
				RXLEV_FULL_DOWNLINK_SUM,
				RXQUAL_FULL_DOWNLINK_CNT,
				RXQUAL_FULL_DOWNLINK_SUM
				)
			SELECT
				DATA_DATE
				,DATA_HOUR
				,INDOOR
				,MOVING
				,TILE_ID
				,BSC_ID
				,CELL_ID
				,BANDINDEX
				,CELL_INDOOR
				,CLUSTER_ID
				,LAC
				,SITE_ID
				,CELL_LON
				,CELL_LAT
				,BCCH_ARFCN
				,CELL_NAME
				,SUM(CALL_CNT) AS CALL_CNT
				,SUM(RXLEV_FULL_DOWNLINK_CNT) AS RXLEV_FULL_DOWNLINK_CNT
				,SUM(RXLEV_FULL_DOWNLINK_SUM) AS RXLEV_FULL_DOWNLINK_SUM
				,SUM(RXQUAL_FULL_DOWNLINK_CNT) AS RXQUAL_FULL_DOWNLINK_CNT
				,SUM(RXQUAL_FULL_DOWNLINK_SUM) AS RXQUAL_FULL_DOWNLINK_SUM
			FROM ',GT_DB,RUN,'.table_tile_start_gsm
			WHERE DATA_HOUR >= ',STARTHOUR,' AND DATA_HOUR < ',ENDHOUR,'
			GROUP BY  
				DATA_DATE
				,DATA_HOUR
				,MOVING
				,INDOOR
				,BSC_ID
				,CELL_ID
				,TILE_ID
			ORDER BY NULL');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
		
	INSERT INTO gt_gw_main.sp_log VALUES(O_GT_DB,'SP_Sub_Generate_Start_GSM','INSERT INTO table_tile_dominant_cell_gsm ', NOW());
	
	SET @SqlCmd=CONCAT('INSERT INTO  ',GT_DB,RUN,'.table_tile_dominant_cell_gsm
				(
				DATA_DATE,
				DATA_HOUR,
				INDOOR,
				MOVING,
				TILE_ID,
				BSC_ID,
				CELL_ID,
				BANDINDEX,
				CELL_INDOOR,
				CLUSTER_ID,
				LAC,
				SITE_ID,
				CELL_LON,
				CELL_LAT,
				BCCH_ARFCN,
				CELL_NAME,
				CALL_CNT,
				RXLEV_FULL_DOWNLINK_CNT,
				RXLEV_FULL_DOWNLINK_SUM,
				RXQUAL_FULL_DOWNLINK_CNT,
				RXQUAL_FULL_DOWNLINK_SUM
				)
			SELECT 
				DATA_DATE,
				DATA_HOUR,
				INDOOR,
				MOVING,
				TILE_ID,
				BSC_ID,
				CELL_ID,
				BANDINDEX,
				CELL_INDOOR,
				CLUSTER_ID,
				LAC,
				SITE_ID,
				CELL_LON,
				CELL_LAT,
				BCCH_ARFCN,
				CELL_NAME,
				CALL_CNT,
				RXLEV_FULL_DOWNLINK_CNT,
				RXLEV_FULL_DOWNLINK_SUM,
				RXQUAL_FULL_DOWNLINK_CNT,
				RXQUAL_FULL_DOWNLINK_SUM
			FROM ',GT_DB,RUN,'.tmp_table_tile_dominant_gsm_',WORKER_ID,';');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	
	INSERT INTO gt_gw_main.sp_log VALUES(O_GT_DB,'SP_Sub_Generate_Start_GSM','INSERT INTO table_tile_dominant_cell_gsm_dy ', NOW());
	SET @SqlCmd=CONCAT('INSERT INTO ',GT_DB,'.table_tile_dominant_cell_gsm_dy
				(
			DATA_DATE,
			INDOOR,
			MOVING,
			TILE_ID,
			BSC_ID,
			CELL_ID,
			BANDINDEX,
			CELL_INDOOR,
			CLUSTER_ID,
			LAC,
			SITE_ID,
			CELL_LON,
			CELL_LAT,
			BCCH_ARFCN,
			CELL_NAME,
			CALL_CNT,
			RXLEV_FULL_DOWNLINK_CNT,
			RXLEV_FULL_DOWNLINK_SUM,
			RXQUAL_FULL_DOWNLINK_CNT,
			RXQUAL_FULL_DOWNLINK_SUM
				)
		 	SELECT
				DATA_DATE
				,INDOOR
				,MOVING
				,TILE_ID
				,BSC_ID
				,CELL_ID
				,BANDINDEX
				,CELL_INDOOR
				,CLUSTER_ID
				,LAC
				,SITE_ID
				,CELL_LON
				,CELL_LAT
				,BCCH_ARFCN
				,CELL_NAME
				,IFNULL(SUM(CALL_CNT),0) AS CALL_CNT
				,IFNULL(SUM(RXLEV_FULL_DOWNLINK_CNT),0) AS RXLEV_FULL_DOWNLINK_CNT
				,IFNULL(SUM(RXLEV_FULL_DOWNLINK_SUM),0) AS RXLEV_FULL_DOWNLINK_SUM
				,IFNULL(SUM(RXQUAL_FULL_DOWNLINK_CNT),0) AS RXQUAL_FULL_DOWNLINK_CNT
				,IFNULL(SUM(RXQUAL_FULL_DOWNLINK_SUM),0) AS RXQUAL_FULL_DOWNLINK_SUM
			FROM ',GT_DB,RUN,'.tmp_table_tile_dominant_gsm_',WORKER_ID,'
			GROUP BY 
				DATA_DATE
				,INDOOR
				,MOVING
				,TILE_ID
				,BSC_ID
				,CELL_ID
			ON DUPLICATE KEY UPDATE
				',GT_DB,'.table_tile_dominant_cell_gsm_dy.CALL_CNT=',GT_DB,'.table_tile_dominant_cell_gsm_dy.CALL_CNT+VALUES(CALL_CNT),
				',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXLEV_FULL_DOWNLINK_CNT=',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXLEV_FULL_DOWNLINK_CNT+VALUES(RXLEV_FULL_DOWNLINK_CNT),
				',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXLEV_FULL_DOWNLINK_SUM=',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXLEV_FULL_DOWNLINK_SUM+VALUES(RXLEV_FULL_DOWNLINK_SUM),
				',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXQUAL_FULL_DOWNLINK_CNT=',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXQUAL_FULL_DOWNLINK_CNT+VALUES(RXQUAL_FULL_DOWNLINK_CNT),
				',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXQUAL_FULL_DOWNLINK_SUM=',GT_DB,'.table_tile_dominant_cell_gsm_dy.RXQUAL_FULL_DOWNLINK_SUM+VALUES(RXQUAL_FULL_DOWNLINK_SUM)
		;');
	PREPARE Stmt FROM @SqlCmd;
	EXECUTE Stmt;
	DEALLOCATE PREPARE Stmt;
	INSERT INTO gt_gw_main.sp_log VALUES(O_GT_DB,'SP_Sub_Generate_Start_GSM',CONCAT('Done: ',TIMESTAMPDIFF(SECOND,START_TIME,SYSDATE()),' seconds.'), NOW());
	
		
END
